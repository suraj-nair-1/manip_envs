<?xml version="1.0" encoding="utf-8"?>
<mujoco>
   <include file="shared_config.xml"></include>
    <worldbody>
        <include file="sawyer_xyz_base.xml"></include>
        <body name="obj0" pos="0 0.6 0.05">
            <geom name="objGeom0" type="box" pos="0 0 0" solimp="0.99 0.99 0.01"
                  size="0.025 0.025 0.025" rgba="0.0 1.0 0.0 1" solref="0.01 1"
                  contype="1" conaffinity="1" condim="4" 
            />
            <joint name="objGeom0_x" type="slide" pos="0 0 0" axis="1 0 0" range="-0.3 0.3" damping="100"/>
            <joint name="objGeom0_y" type="slide" pos="0 0 0" axis="0 1 0" range="-0.3 0.3" damping="100"/>
            <joint name="objGeom0_z" type="slide" pos="0 0 0" axis="0 0 1" range="-0 0.1" damping="100"/>
            <inertial pos="0 0 0" mass="10" diaginertia="0.001 0.001 0.001"/>
        </body>

        <body name="obj1" pos="0 0.6 0.05">
            <geom name="objGeom1" type="box" pos="0 0 0" solimp="0.99 0.99 0.01"
                  size="0.025 0.025 0.025" rgba="1 0.5 1 1" solref="0.01 1"
                  contype="1" conaffinity="1" condim="4" 
            />
            <joint name="objGeom1_x" type="slide" pos="0 0 0" axis="1 0 0" range="-.3 0.3" damping="100"/>
            <joint name="objGeom1_y" type="slide" pos="0 0 0" axis="0 1 0" range="-0.3 0.3" damping="100"/>
            <joint name="objGeom1_z" type="slide" pos="0 0 0" axis="0 0 1" range="-0 0.1" damping="100"/>
            <inertial pos="0 0 0" mass="10" diaginertia="0.001 0.001 0.001"/>
        </body>
      
      <body name="obj2" pos="0 0.6 0.05">
            <geom name="objGeom2" type="box" pos="0 0 0" solimp="0.99 0.99 0.01"
                  size="0.025 0.025 0.025" rgba="0 0.0 1 1" solref="0.01 1"
                  contype="1" conaffinity="1" condim="4" 
            />
            <joint name="objGeom2_x" type="slide" pos="0 0 0" axis="1 0 0" range="-.3 0.3" damping="100"/>
            <joint name="objGeom2_y" type="slide" pos="0 0 0" axis="0 1 0" range="-.3 0.3" damping="100"/>
            <joint name="objGeom2_z" type="slide" pos="0 0 0" axis="0 0 1" range="-.3 0.3" damping="100"/>
            <inertial pos="0 0 0" mass="10" diaginertia="0.001 0.001 0.001"/>
        </body>

        <camera mode="fixed" name="cam0" pos="-0.0 1.42 0.85" quat="0.924 -0.383 0 0"/>
      

        <body name="door" pos="0.1 0.85 0.1">
                <joint name="doorjoint" pos="-.16 0 0" axis="0 0 1"
                       type="hinge" limited="true" range="-1.5708 1.5708" damping="1" armature="0" />
                <geom type="box" pos="0 0 0" size="0.12 0.01 0.1"
                      rgba=".1 .1 .9 1" contype="1" conaffinity="1" material="light_wood"/>
                <!--front, parallel to door-->
                <geom type="box" pos="0.1 -0.15 0.05" size="0.04 0.01 0.02" rgba="0 0 0 1" contype="1" conaffinity="1" name="handle" material="light_wood"/>
                <!--front-->
                <geom type="box" pos="0.14 -0.075 0.05" size="0.01 0.075 0.02" rgba="0 0 0 1" contype="1" conaffinity="1" material="light_wood"/>
        </body>

    </worldbody>

